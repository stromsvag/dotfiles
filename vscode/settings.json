{
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
      {
        "before": ["j", "k"],
        "after": ["<Esc>"]
      }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["<leader>", "d"],
        "after": ["d", "d"]
      },
      {
        "before": ["<leader>", "w"],
        "commands": [
          ":w"
        ]
      },
      {
        "before": ["<leader>", "<CR>"],
        "commands": [":nohl"]
      }
    ],
    "vim.leader": ",",
    "vim.handleKeys": {
      "<C-a>": false,
      "<C-f>": false
    },
    "editor.fontLigatures": true
  }